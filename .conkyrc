background no
use_xft yes
xftfont ubuntu mono:size=10
xftalpha 1
update_interval 1.0
total_run_times 0
own_window yes
own_window_transparent no
own_window_argb_visual yes
own_window_argb_value 200
own_window_type override
own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager
own_window_colour 0F002C
double_buffer yes
minimum_size 1 1
maximum_width 280
draw_shades no
draw_outline no
draw_borders no
border_width 4
draw_graph_borders yes
default_color 000000
default_outline_color 111111
text_buffer_size 1024
alignment top_right
gap_x 8
gap_y 0
no_buffers yes
uppercase no
cpu_avg_samples 2
override_utf8_locale no
color1 aaaaaa
color2 9000cc
color3 86c113
color4 1a3f88
color5 00c5c5
color6 666666

#darkpurple 0F002C
#lightpurple 9000cc
#green 86c113
#darkblue 1a3f88
#lightblue 00c5c5

##############################################
#  Output
##############################################
TEXT
${color6}${font ubuntu:bold:size=11}Time${hr 2}
${font ubuntu:size=15}${color5}${alignc}${time %I:%M:%S %p}
${font ubuntu:size=14}${color2}${execpi 300 cal -h | sed "s/\<[0-9]\{4\}\>\s*/&\${color3}\${font ubuntu mono:size=12}\n/" | sed "s/^/\${alignc}/g" | sed "s/\<$(date +%-d)\>/\${color5}&\${color4}/"}

${color6}${font ubuntu:bold:size=11}System${hr 2}
${font}${color2}$sysname $kernel $alignr $machine
Host:$alignr$nodename
Uptime:$alignr$uptime

${color6}${font ubuntu:bold:size=11}Top Processes${hr 2}${font}
${color2}${top_mem pid 1} ${color1}${top_mem name 1}${color5}${top_mem mem 1}%${alignr}${color3}${top_mem mem_res 1}
${color2}${top_mem pid 2} ${color1}${top_mem name 2}${color5}${top_mem mem 2}%${alignr}${color3}${top_mem mem_res 2}
${color2}${top_mem pid 3} ${color1}${top_mem name 3}${color5}${top_mem mem 3}%${alignr}${color3}${top_mem mem_res 3}
${color2}${top_mem pid 4} ${color1}${top_mem name 4}${color5}${top_mem mem 4}%${alignr}${color3}${top_mem mem_res 4}
${color2}${top_mem pid 5} ${color1}${top_mem name 5}${color5}${top_mem mem 5}%${alignr}${color3}${top_mem mem_res 5}
${color2}${top_mem pid 6} ${color1}${top_mem name 6}${color5}${top_mem mem 6}%${alignr}${color3}${top_mem mem_res 6}
${color2}${top_mem pid 7} ${color1}${top_mem name 7}${color5}${top_mem mem 7}%${alignr}${color3}${top_mem mem_res 7}
${color2}${top_mem pid 8} ${color1}${top_mem name 8}${color5}${top_mem mem 8}%${alignr}${color3}${top_mem mem_res 8}
${color2}${top_mem pid 9} ${color1}${top_mem name 9}${color5}${top_mem mem 9}%${alignr}${color3}${top_mem mem_res 9}

${color6}${font ubuntu:bold:size=11}Status${hr 2}
${font}${color4}RAM: $alignr${color3}$mem/$memmax
${color2}${memgraph 24,279 00c5c5 00c5c5}
${voffset -11}${membar}
${color4}CPU: $alignr${color3}${cpu cpu0}%
${color2}${cpugraph cpu0 24,279 00c5c5 00c5c5}
${voffset -11}${cpubar cpu0}
${color4}CPU 1: $alignr${color3}${cpu cpu1}%
${color2}${cpubar cpu1}
${color4}CPU 2: $alignr${color3}${cpu cpu2}%
${color2}${cpubar cpu2}
${if_running deadbeef-gtkui}
${color6}${font ubuntu:bold:size=11}Music${hr 2}
${color5}${font}${font ubuntu:size=15}${alignc}${execpi 5 deadbeef --nowplaying "%t" | fold -w 28 -s | sed 's/^/${alignc}/g'}
${color3}${font ubuntu:size=11}${execpi 10 deadbeef --nowplaying "%a" | fold -w 35 -s | sed 's/^/${alignc}/g'}
${color1}${font ubuntu:size=10}${execpi 10 deadbeef --nowplaying "%b" | fold -w 40 -s | sed 's/^/${alignc}/g'}
${font}${color4}Track ${execi 5 deadbeef --nowplaying "%n"}:${alignr}${color3}${exec deadbeef --nowplaying "%e / %l"}
${color2}${execbar ~/Scripts/deadbeefProgress.sh}
${else}
${color6}${font ubuntu:bold:size=11}Cow Says${hr 2}
${color1}${font}${execi 1800 ~/Scripts/getQuote.sh | sed "s/^/ /g"}
${endif}










